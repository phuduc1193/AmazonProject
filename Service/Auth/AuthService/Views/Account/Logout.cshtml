@model LogoutViewModel

<div class="logout-page">
    <div class="page-header">
        <h3>Logout</h3>
    </div>

    <div class="row">
        <div class="col-sm-6">
            <h5 class="py-3 text-muted">Would you like to logout of Identity Server?</h5>
            <form asp-action="Logout">
                <input type="hidden" name="logoutId" value="@Model.LogoutId"  />
                <fieldset>
                    <div class="form-group">
                        <button class="btn btn-primary px-4">Yes</button>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</div>